{
  "name": "GranumWiki",
  "version": "0.8.3",
  "description": "A personal distributed Wiki",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "build-electron": "npm install && electron-rebuild --pre-gyp-fix",
    "pack": "build",
    "dist": "build -d"
  },
  "build": {
    "app-bundle-id": "com.dmitryilin.granumwiki",
    "app-category-type": "public.app-category.productivity"
  },
  "keywords": [
    "Granum",
    "Wiki",
    "Electron"
  ],
  "author": {
    "name": "Dmitry Ilin",
    "email": "i@dmitryilin.com",
    "url": "http://dmitryilin.com"
  },
  "license": "GPL-2.0",
  "devDependencies": {
    "electron-builder": "^2.8.6",
    "electron-prebuilt": "^0.36.12",
    "electron-rebuild": "^1.1.3"
  },
  "dependencies": {
    "cote": "0.8.4",
    "crypto": "0.0.3",
    "electron-settings": "^1.0.2",
    "file-type": "^3.8.0",
    "log4js": "^0.6.33",
    "markdown-it-anchor": "^2.5.0",
    "mkdirp": "^0.5.1",
    "randomstring": "^1.1.4",
    "sequelize": "^3.19.3",
    "sqlite3": "^3.1.1"
  }
}
